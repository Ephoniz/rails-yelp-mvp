<h1>New Review</h1>

<%=%>

<%= form_for [@restaurant, @review] do |f| %>
  <div class="form-inputs">
    Content: <%= f.text_field :content %>
    Rating: <%= f.number_field :rating%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<%= link_to 'Back', restaurant_path(@restaurant) %>